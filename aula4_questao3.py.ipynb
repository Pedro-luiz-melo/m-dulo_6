    "\n",
    "# Compreens√£o de lista para calcular os pagamentos\n",
    "pagamentos = [\n",
    "    ganho_por_hora * min(h, 40) + hora_extra * max(0, h - 40)\n",
    "    for h in horas_trabalhadas\n",
    "]\n",
    "\n",
    "print(pagamentos)\n"
   ]
